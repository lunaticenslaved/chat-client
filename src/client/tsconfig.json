{
    "extends": [
        "../../tsconfig.base.json"
    ],
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "moduleResolution": "Bundler",
        "noEmit": true,
        "types": ["node", "vite/client", "vite-plugin-svgr/client"],
        "jsx": "react-jsx",

        /* Paths */
        "baseUrl": ".",
        "rootDir": "..",
        "paths": {
            "@/*": ["./*"],
            "@/common/*": ["../common/*"]
        }
    },
}
  