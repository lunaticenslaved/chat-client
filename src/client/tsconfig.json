{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "lib": ["DOM", "es2019", "esnext"],
    "module": "esnext",
    "moduleResolution": "Bundler",
    "types": ["vite/client", "vite-plugin-svgr/client"],
    "jsx": "react-jsx",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@store": ["../store/index.ts"],
      "@domain/*": ["../domain/*"],
      "@api/*": ["../api/*"]
  }
  },
  "exclude": ["../server"]
}
