{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx nodemon src/server/index.ts --project src/server/tsconfig.json",
    "start": "APP_ENV=prod node dist/server/index.js",
    "build:client": "vite build --config vite.client.config.ts && vite build --config vite.server.config.ts",
    "build:server": "tsc --project src/server/tsconfig.json && tsc-alias --project src/server/tsconfig.json --dir dist/server",
    "build": "rm -rf dist && npm run build:client && npm run build:server",
    "lint": "eslint . --ext ts,tsx --max-warnings 0 && prettier --check '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}'",
    "lint:fix": "eslint . --ext ts,tsx --fix && prettier --write '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}' --log-level warn"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.11.3",
    "@fingerprintjs/fingerprintjs": "^4.2.1",
    "@lunaticenslaved/schema": "^0.0.54",
    "@prisma/client": "^5.6.0",
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "^5.11.0",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "emoji-mart": "^5.5.2",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "prisma": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-use": "^17.4.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tinycolor2": "^1.6.0",
    "uninstall": "^0.0.0",
    "web-vitals": "^2.1.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@lunaticenslaved/configs": "^0.0.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.18.8",
    "@types/react-dom": "^18.2.14",
    "@types/tinycolor2": "^1.4.5",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.1",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-svgr": "^4.2.0"
  }
}
