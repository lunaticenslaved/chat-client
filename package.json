{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && tsc -p src/server/tsconfig.json && tsc-alias -p src/server/tsconfig.json && vite build",
    "lint": "eslint . --ext ts,tsx && prettier --check '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}'",
    "lint:fix": "eslint . --ext ts,tsx --fix && prettier --write '**/*.{js,jsx,ts,tsx,css,scss,json,yaml,yml,md}' --log-level warn"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@lunaticenslaved/schema": "^0.0.43",
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "^5.11.0",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "emoji-mart": "^5.5.2",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "tinycolor2": "^1.6.0",
    "uninstall": "^0.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@lunaticenslaved/configs": "^0.0.1",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.18.8",
    "@types/react-dom": "^18.2.14",
    "@types/tinycolor2": "^1.4.5",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "node-sass": "^9.0.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "typescript": "5.1",
    "vite": "^5.0.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-svgr": "^4.2.0"
  }
}
